{
  "env": {
    "node": true,
    "commonjs": true,
    "es2020": true
  },
  "extends": ["plugin:prettier/recommended", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "prettier/prettier": "off",
    "no-console": "off",
    "no-unused-vars":"off"
  },
  "plugins": ["prettier"]
}
